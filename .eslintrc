{
    "extends": "@react-native",
    "rules": {
        "quotes": [2, "double", {"avoidEscape": true}]
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2020,  // or the version you're using
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true  // Enable JSX
        }
    },
    "plugins": ["react"]
}

